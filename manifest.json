{
  "manifest_version": 2,
  "name": "trueSelf",
  "version": "1.0",
  "description": "Replaces the text 'Ruby' with 'R.J.'",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
